{
    "name": "coxy2001/silverstripe-basic",
    "type": "silverstripe-recipe",
    "description": "Basic SilverStripe website installer",
    "require": {
        "php": "^7.3",
        "silverstripe/recipe-cms": "4.7.0@stable",
        "silverstripe/framework": "4.7.0",
        "silverstripe/login-forms": "^4.5",
        "silverstripe/elemental-fileblock": "^2.0",
        "silverstripe/elemental-bannerblock": "^2.0",
        "dnadesign/silverstripe-elemental-userforms": "3.x",
        "axllent/silverstripe-scss": "^1.6",
        "tractorcow/silverstripe-colorpicker": "^4@dev"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7",
        "sminnee/phpunit-mock-objects": "^3.4.5"
    },
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/database.yml",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
